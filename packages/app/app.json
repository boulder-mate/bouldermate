{
  "expo": {
    "name": "BoulderMate",
    "slug": "bouldermate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "/assets/images/logo-whitebg.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow BoulderMate to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow BoulderMate to access your camera roll."
        }
      ]
    ],
    "splash": {
      "image": "./assets/images/logo-whitebg.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-whitebg.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/logo-whitebg.png"
    },
    "extra": {
      "eas": {
        "projectId": "29d7fe9c-c4f6-44ea-9d1d-e1c4c5aca522"
      }
    }
  }
}
